    xmlns:tools="http://schemas.android.com/tools">


    <factor.labs.indiancalendar.CalendarUI.CalendarDialogUI.core.internal.MDButton
        android:id="@+id/buttonDefaultNeutral"
        style="@style/MD_ActionButton.Text"
        tools:text="Neutral" />

    <factor.labs.indiancalendar.CalendarUI.CalendarDialogUI.core.internal.MDButton
